{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug x402_mock (main.py)",
      "type": "debugpy",
      "request": "launch",
      "console": "integratedTerminal",
      "justMyCode": true,
      "module": "main"
    },
    {
      "name": "Debug server example",
      "type": "debugpy",
      "request": "launch",
      "console": "integratedTerminal",
      "justMyCode": true,
      "module": "example.server_example"
    },
    {
      "name": "Debug client example",
      "type": "debugpy",
      "request": "launch",
      "console": "integratedTerminal",
      "justMyCode": true,
      "module": "example.client_example"
    },
    {
      "name": "Debug Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "cwd": "${fileDirname}",
      "justMyCode": true
    }
  ]
}